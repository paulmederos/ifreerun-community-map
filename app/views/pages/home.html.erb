<section class="clearfix">
	<div class="sixteen columns">	
		<h1>Where do you want to run?</h1>
	</div>
	<div class="four columns" style="background:rgba(150,0,0,.2);">
		<h2>List of Users</h2>
		<ul>
		<% @users.each do |u| %>
			<li><%= u.email %></li>
		<% end %>
		</ul>
	</div>
	<div class="six columns" style="background:red;">
		<h2>List of Communities</h2>
		<%= link_to "New Community", "#new_community_path" %>
	</div>
</section>