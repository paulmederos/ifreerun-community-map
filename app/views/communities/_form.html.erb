<%= form_for @community do |f| %>
	<p>
    <%= f.label :name, "Community Name" %>
    <%= f.text_field :name, :placeholder => "ex. Virginia Tech Parkour" %>
  </p>

	<%= f.fields_for :location do |loc| %>
		<p>
	    <%= loc.label :address, "Location (City, State) of Community" %>
	    <%= loc.text_field :address %>
	  </p>
	<% end %>

  <p>
    <%= f.label :url, "Website URL" %>
    <%= f.text_field :url, :placeholder => "ex. http://ifreerun.com" %>
  </p>
  <p>
    <%= f.label :summary %>
    <%= f.text_area :summary %>
  </p>
  <p><%= f.submit %></p>
<% end %>
